{
  "name": "app-server",
  "version": "1.0.0",
  "description": "app server",
  "private": true,
  "dependencies": {
    "egg": "^2.10.0",
    "egg-jwt": "^3.1.2",
    "egg-mysql": "^3.0.0",
    "egg-passport": "^2.0.1",
    "egg-redis": "^2.0.0",
    "egg-scripts": "^2.9.1",
    "egg-sequelize": "^4.1.0",
    "egg-session-redis": "^2.0.1",
    "egg-validate": "^1.1.1",
    "lodash": "^4.17.10",
    "md5": "^2.2.1",
    "mysql2": "^1.6.1",
    "passport-local": "^1.0.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.8.3",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.20.0",
    "eslint": "^5.5.0",
    "eslint-config-egg": "^7.1.0",
    "sequelize-cli": "^4.1.1",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-app-server",
    "stop": "egg-scripts stop --title=egg-server-app-server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "sql": "node ./sql/index.js",
    "db:init": "npx sequelize migration:generate --name",
    "db:up": "npx sequelize db:migrate",
    "db:down": "npx sequelize db:migrate:undo",
    "db:down-all": "npx sequelize db:migrate:undo:all"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "ruiyong.lee@gmail.com",
  "license": "MIT"
}
